@model List<RapidApiConsume.Models.ApiMovieViewModel>

@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class="container">
    <br> <br>
    <table class="table table-bordered">
        <tr>
            <th>#</th>
            <th>Film adi</th>
            <th>Puani</th>
            <th>Fragman</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.rank</td>
                <td>@item.title</td>
                <td>@item.rating</td>
                <td><a href="#" class="btn btn-dark"> Fragmani Oynat</a></td>
            </tr>
            
        }

    </table>
    
</div>